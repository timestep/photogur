<div class="row">
	<div class="span">
		<%= @picture[:title] %>, <%= @picture[:artist] %>
	</div>
	
</div>
<div class="row">
	<div class="span">
		<%= image_tag @picture.url %>
	</div>
</div>
<div class="row">
	<div class="span">
		<%= link_to "Back to Pictures", pictures_url, class: "btn btn-warning" %>
	</div>
	<div class="offset10">
	<%= link_to "Edit", edit_picture_path(@picture), class: "btn btn-primary" %><%= link_to "Delete", @picture, class: "btn btn-danger", :confirm => "Are you sure you want to delete this picture?", :method => :delete %>
	</div>
</div>




